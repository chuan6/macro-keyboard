{
    "manifest_version": 2,
    
    "name": "Macro Keyboard",
    "description": "A virtual keyboard that allows customizations of the expansion of each key.",
    "version": "0.1",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "permissions": [
        "clipboardRead",
        "clipboardWrite"
    ]
}
